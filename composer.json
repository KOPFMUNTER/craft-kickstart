{
    "name": "kopfmunter/craft-kickstart",
    "description": "Craft CMS Scaffolding Project",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "project"
    ],
    "license": "Unlicense",
    "homepage": "https://kopfmunter.de",
    "type": "project",
    "support": {
        "email": "support@craftcms.com",
        "issues": "https://github.com/craftcms/cms/issues",
        "forum": "https://craftcms.stackexchange.com/",
        "source": "https://github.com/craftcms/cms",
        "docs": "https://craftcms.com/docs",
        "rss": "https://craftcms.com/changelog.rss"
},
  "require": {
        "craftcms/cms": "^3.0.0",
        "craftcms/redactor": "^2.3",
        "doublesecretagency/craft-cpcss": "^2.1",
        "mmikkel/cp-field-inspect": "^1.0",
        "nystudio107/craft-seomatic": "^3.0.0",
        "putyourlightson/craft-dashboard-begone": "^1.0",
        "solspace/craft3-freeform": "^2.5",
        "verbb/knock-knock": "^1.0",
        "vlucas/phpdotenv": "^2.4.0"
    },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php craft setup",
      "@php km-setup"
    ]
  }
}
